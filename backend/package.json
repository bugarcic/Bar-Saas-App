{
  "name": "backend",
  "version": "1.0.0",
  "description": "BarSaas Backend API",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:init": "node scripts/init-db.js",
    "pdf:inspect": "node scripts/inspect-pdf.js",
    "pdf:map": "node scripts/generate-visual-map.js",
    "pdf:test-fill": "node scripts/test-fill-page1.js",
    "pdf:test-fill-2": "node scripts/test-fill-page2.js",
    "pdf:test-fill-3": "node scripts/test-fill-page3.js",
    "types:generate": "ts-node scripts/generate-types.ts",
    "dev": "ts-node server.ts",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "node -e \"const fs=require('fs');const path=require('path');['templates','mappings'].forEach(dir=>{const src=path.join(__dirname,dir);const dest=path.join(__dirname,'dist',dir);if(!fs.existsSync(dest))fs.mkdirSync(dest,{recursive:true});fs.readdirSync(src).forEach(f=>fs.copyFileSync(path.join(src,f),path.join(dest,f)))});\"",
    "start": "node dist/server.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
